{
  "name": "wouter-preact",
  "version": "2.11.0",
  "description": "A minimalistic routing for React and Preact (Preact-only version).",
  "type": "module",
  "keywords": [
    "react",
    "preact",
    "router",
    "tiny",
    "routing",
    "hooks",
    "useLocation"
  ],
  "files": [
    "esm",
    "types/**/*.d.ts",
    "types/*.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./esm/index.js"
    },
    "./use-location": {
      "types": "./types/use-location.d.ts",
      "default": "./esm/use-location.js"
    },
    "./matcher": {
      "types": "./types/matcher.d.ts",
      "default": "./esm/matcher.js"
    },
    "./static-location": {
      "types": "./types/static-location.d.ts",
      "default": "./esm/static-location.js"
    }
  },
  "types": "types/index.d.ts",
  "typesVersions": {
    "*": {
      "types/index.d.ts": [
        "types/index.d.ts"
      ],
      "use-location": [
        "types/use-location.d.ts"
      ],
      "matcher": [
        "types/matcher.d.ts"
      ],
      "static-location": [
        "./types/static-location.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rollup -c"
  },
  "author": "Alexey Taktarov <molefrog@gmail.com>",
  "repository": "molefrog/wouter",
  "license": "ISC",
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "wouter": "latest"
  }
}
