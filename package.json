{
  "name": "wouter",
  "version": "1.2.0",
  "description": "A minimalistic routing for React. Nothing extra, just HOOKS.",
  "keywords": [
    "react",
    "router",
    "tiny",
    "routing",
    "hooks"
  ],
  "scripts": {
    "test": "jest --verbose --coverage",
    "size": "size-limit"
  },
  "author": "Alexey Taktarov <molefrog@gmail.com>",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "repository": "molefrog/wouter",
  "size-limit": [
    {
      "path": "index.js",
      "limit": "1.5 KB"
    }
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-react-jsx"
    ]
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.3",
    "eslint": "^5.16.0",
    "jest": "^24.7.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.0",
    "size-limit": "^1.0.1"
  }
}
